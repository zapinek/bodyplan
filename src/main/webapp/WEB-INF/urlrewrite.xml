<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 3.0//EN" "http://tuckey.org/res/dtds/urlrewrite3.0.dtd">
<urlrewrite default-match-type="wildcard">
	<rule>
		<from>/robots.txt</from>
		<to>/robots.txt</to>
	</rule>
	<rule>
		<from>/fonts/**</from>
		<to>/fonts/$1</to>
	</rule>
	<rule>
		<from>/img/**</from>
		<to>/img/$1</to>
	</rule>
	<rule>
		<from>/images/**</from>
		<to>/images/$1</to>
	</rule>
	<rule>
		<from>/js/**</from>
		<to>/js/$1</to>
	</rule>
	<rule>
		<from>/css/**</from>
		<to>/css/$1</to>
	</rule>
	<rule>
		<from>/plugins/**</from>
		<to>/plugins/$1</to>
	</rule>
	<rule>
		<from>/assets/**</from>
		<to>/assets/$1</to>
	</rule>
	<rule>
		<from>/services/**</from>
		<to>/services/$1</to>
	</rule>
	<rule>
		<from>/favicon.ico</from>
		<to>/favicon.ico</to>
	</rule>
	<rule>
		<from>/image/**.png</from>
		<to>/web/image.html?username=$1</to>
	</rule>
	<rule>
		<from>/</from>
		<to>/web</to>
	</rule>
	<rule>
		<from>/**</from>
		<to>/web/$1</to>
	</rule>
	<outbound-rule>
		<from>/web/**</from>
		<to>/$1</to>
	</outbound-rule>
</urlrewrite>